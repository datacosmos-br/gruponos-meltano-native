# Oracle WMS Source Configuration
TAP_ORACLE_WMS_BASE_URL=https://wms.example.com/api
TAP_ORACLE_WMS_USERNAME=your_username
TAP_ORACLE_WMS_PASSWORD=your_password
TAP_ORACLE_WMS_COMPANY_CODE=COMPANY
TAP_ORACLE_WMS_FACILITY_CODE=FACILITY

# Oracle Analytics Target Configuration
FLEXT_TARGET_ORACLE_HOST=your.oracle.host
FLEXT_TARGET_ORACLE_PORT=1522
FLEXT_TARGET_ORACLE_SERVICE_NAME=your_service_name
FLEXT_TARGET_ORACLE_USERNAME=your_db_user
FLEXT_TARGET_ORACLE_PASSWORD=your_db_password
FLEXT_TARGET_ORACLE_PROTOCOL=tcps
FLEXT_TARGET_ORACLE_TABLE_PREFIX=WMS_

# Optional Performance Settings
TAP_ORACLE_WMS_ENABLE_INCREMENTAL=true
FLEXT_TARGET_ORACLE_SSL_DN_MATCH=false