# Test environment variables for configuration migration validation
# These are not real credentials - just for testing the configuration system

# Required core connection settings
TAP_ORACLE_WMS_BASE_URL=https://test-wms-instance.com
TAP_ORACLE_WMS_USERNAME=test_user
TAP_ORACLE_WMS_PASSWORD=test_pass

# Required Oracle target settings
FLEXT_TARGET_ORACLE_HOST=test-oracle-host
FLEXT_TARGET_ORACLE_SERVICE_NAME=test_service
FLEXT_TARGET_ORACLE_USERNAME=test_oracle_user
FLEXT_TARGET_ORACLE_PASSWORD=test_oracle_pass
FLEXT_TARGET_ORACLE_PROTOCOL=TCP

# WMS API configuration defaults (should be picked up from config.py)
WMS_API_VERSION=v10
WMS_PAGE_MODE=sequenced
WMS_ORDERING=mod_ts
WMS_INCREMENTAL_ORDERING=mod_ts
WMS_REPLICATION_KEY=mod_ts

# Performance settings for full sync
WMS_PAGE_SIZE=100
WMS_REQUEST_TIMEOUT=3600
WMS_BATCH_SIZE_ROWS=100
WMS_MAX_SYNC_DURATION=10800

# Performance settings for incremental sync
WMS_INCREMENTAL_PAGE_SIZE=100
WMS_INCREMENTAL_REQUEST_TIMEOUT=2700
WMS_INCREMENTAL_MAX_SYNC_DURATION=5400

# Reliability settings
WMS_MAX_RETRIES=3
WMS_RETRY_WAIT_MULTIPLIER=2.0
WMS_RETRY_MAX_WAIT=60

# Incremental sync settings
WMS_LOOKBACK_MINUTES=30
WMS_INCREMENTAL_OVERLAP_MINUTES=30

# Cache settings
WMS_CATALOG_CACHE_TTL=3600
WMS_SCHEMA_CACHE_TTL=3600

# Security settings
WMS_OAUTH_SCOPE=wms.read